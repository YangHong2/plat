<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dhlk.subcontract.dao.ProjectProgressDao">
    <resultMap type="com.dhlk.entity.sub.ProjectProgress" id="ProjectProgressMap">
        <id column="id" property="id"/>
        <result column="project_id" property="projectId"/>
        <result column="plan" property="plan"/>
        <result column="project_described" property="projectDescribed"/>
        <result column="path" property="path"/>
        <result column="create_Time" property="createTime"/>
    </resultMap>


    <select id="findAll" parameterType="_integer" resultMap="ProjectProgressMap">
        SELECT * FROM project_progress where project_id =#{projectId}
    </select>

    <insert id="addOne" parameterType="com.dhlk.entity.sub.ProjectProgress">
        insert into project_progress(id,project_id,plan,project_described,path,create_time)
        values (null,#{projectId},#{plan},#{projectDescribed},#{path},#{createTime})
    </insert>
</mapper>

